                            <table style="display: inline;">
                                <tr>
                                    <td>
                                        <h5><apex:outputLabel value="{!$Label.CRABB_DEV__Name} : "/></h5>
                                    </td>
                                    <td align="right">
                                        <apex:outputField value="{!myTestata.Nome__c}" />
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <h5><apex:outputLabel value="{!$Label.CRABB_DEV__Status} : "/></h5>
