                                                        </td>
                                                        <td>
                                                            <apex:outputText value="{!cdi.Name}"/>
                                                        </td>
                                                        <td>
                                                            <apex:outputText value="{!cdi.Nome__c}"/>
                                                        </td>
                                                        <td>
                                                            <apex:outputText value="{!cdi.Descrizione__c}"/>
                                                        </td>
                                                    </tr>
                                                </apex:repeat>
