                    function callMethodNew(){
                        var prof = document.getElementsByClassName('newProf')[0].value;
                        var pu = $('#newPU')[0].checked;
                        var ap = $('#newAP')[0].checked;
                        var ca = $('#newCA')[0].checked;
                        var cs = $('#newCS')[0].checked;
                        var ec = $('#newEC')[0].checked;
                        var cp = $('#newCP')[0].checked;
                        var cc = $('#newCC')[0].checked;
                        var fc = $('#newFC')[0].checked;
                        var ac = $('#newAC')[0].checked;
                        var go = $('#newGO')[0].checked;
                        var el = $('#newEL')[0].checked;
                        var cm = $('#newCM')[0].checked;
                        var gca = $('#newGCA')[0].checked;
                        var is = $('#newIS')[0].checked;
                        var fsc = $('#newFSC')[0].checked;

                        Visualforce.remoting.Manager.invokeAction(
                            '{!$RemoteAction.GestioneAmministrazioneCTRL.addNewRecord}',
                            prof,
                            pu,
                            ap,
                            ca,
                            cs,
                            ec,
                            cp,
                            cc,
                            fc,
                            ac,
                            go,
                            el,
                            cm,
                            gca,
                            is,
                            fsc,
                            function(result, event){
                                if(result == null || result == '' || result == 'error'){
                                    return;
                                }

                                window.location = result;
                            }
                        );
